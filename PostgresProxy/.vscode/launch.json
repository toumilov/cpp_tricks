{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Build and debug",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/proxy",
			"args": ["6776","127.0.0.1"],
			"environment": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "Build: make"
		}
	]
}